
type Material{
    id: ID!
    name: String!
}

type Room {
    id: ID!
    capacity: Int!
    name: String!
    forStaff: Boolean!
    contains: [Material]
}

type Query {
    rooms: [Room]
    room(id: ID!): Room
    material(id: ID!): Material
    materials: [Material]
}

type Mutation {
    createMaterial(name: String!): Material
}

